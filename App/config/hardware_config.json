{
  "pixhawk": {
    "hardware": {
      "model": "PX4 PIX 2.4.8",
      "main_outputs": 8,
      "aux_outputs": 6,
      "pwm_rate_hz": 333
    },
    "servo_channels": {
      "fin_front_left": 1,
      "fin_front_right": 3,
      "fin_rear_left": 4,
      "fin_rear_right": 5,
      "main_motor": 6
    },
    "pwm_limits": {
      "min": 1000,
      "neutral": 1500,
      "max": 2000
    }
  },
  "sensors": {
    "d300_depth": {
      "enabled": true,
      "interface": "i2c",
      "address": "0x76",
      "update_rate_hz": 10
    },
    "imu": {
      "enabled": true,
      "source": "mavlink"
    },
    "gps": {
      "enabled": true,
      "source": "mavlink"
    }
  },
  "raspberry_pi": {
    "gpio": {
      "buzzer": 7,
      "control_button": 13,
      "led_red": 4,
      "led_green": 5,
      "led_blue": 6,
      "warning_led": 8,
      "system_status_led": 10
    },
    "i2c": {
      "bus_number": 1,
      "depth_sensor_address": "0x76"
    }
  },
  "mavlink": {
    "connection_string": "/dev/ttyACM0",
    "baudrate": 115200,
    "timeout": 5
  },
  "network": {
    "web_gui_port": 5000,
    "log_level": "INFO"
  }
} 